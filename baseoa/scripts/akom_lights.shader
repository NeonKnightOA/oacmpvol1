
textures/akom_lights/akomlight2_200
{
	qer_editorimage textures/akom_lights/akomlight2_200.tga
	q3map_surfacelight 200
	surfaceparm nomarks
	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/akom_lights/akomlight2_200.tga
		blendFunc GL_DST_COLOR GL_ZERO
		rgbGen identity
	}
}

