textures/akom_lights/fireflies
{
	qer_editorimage textures/akom_lights/fireflies.tga
	q3map_surfacelight 2500
	surfaceparm nomarks
	surfaceparm nonsolid
	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/akom_lights/fireflies.tga
		tcMod turb 0 0.25 0 0.5
                tcmod scroll 1 1
		blendfunc GL_ONE GL_ONE
		rgbGen identity
	}
}